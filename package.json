    "browserify": "^10.2.0",
    "cssnext": "^1.4.0",
    "nodemon": "^1.3.7",
    "postcss": "^4.1.11",
    "standard": "^3.9.0",
    "watchify": "^3.2.1"
  },
  "dependencies": {
    "babyparse": "^0.4.3",
    "d3": "^3.5.5",
    "domready": "^1.0.8",
